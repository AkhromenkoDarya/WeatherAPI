<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>9.0</LangVersion>
  </PropertyGroup>

  <PropertyGroup>
    <Version>1.0.0.1</Version>
    <PackageReleaseNotes>Запуск проекта</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>WeatherAPIClient</PackageId>
    <Authors>otulissa2012@yandex.ru</Authors>
    <Description>Клиент для WebAPI онлайн-сервиса погоды WeatherAPI</Description>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>WeatherApiClient.snk</AssemblyOriginatorKeyFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/AkhromenkoDarya/WeatherAPI</PackageProjectUrl>
    <RepositoryUrl>https://github.com/AkhromenkoDarya/WeatherAPI.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Weather, WebAPI</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EnumExtensions.System.Text.Json" Version="1.1.0" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="5.0.0" />
    <PackageReference Include="System.Net.Http.Json" Version="5.0.0" />
  </ItemGroup>

</Project>
